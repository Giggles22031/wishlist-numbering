{
    "Steam-WebKit": "path/to/webkit.css",
    "Patches": [
        {
            "MatchRegexString": "https://.*.steampowered.com",
            "TargetCss": "webkit.css",
            "TargetJs": "wishlist-numbering.js"
        },
        {
            "MatchRegexString": ".*wishlist.*",
            "TargetJs": "wishlist-numbering.js"
        }
    ],
    "UseDefaultPatches": true,
    "author": "Giggles",
    "description": "Theme made for Zerkaa to help with the counting for the wishlist series",
    "name": "wishlist-numbering",
    "version": "1.0.0", 
    "github": {
        "owner": "Giggles22031",
        "repo_name": "https://github.com/Giggles22031/wishlist-numbering"
    },
    "tags": [
        "wishlist", "numbering"
    ],
    "header_image": "http://site.com/path/to/thumbnail.png",
    "splash_image": "http://site.com/path/to/thumbnail.png"
}
